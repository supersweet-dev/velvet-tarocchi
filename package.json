{
	"name": "velvet-tarocchi",
	"description": "A simple 3 card spread tarot reading. Uses the user's birthday to 'seed' card draws.",
	"author": "supersweet",
	"version": "1.2.0",
	"private": false,
	"main": "dist/index.js",
	"module": "dist/index.js",
	"files": [
		"dist",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/supersweet-dev/velvet-tarocchi.git"
	},
	"dependencies": {
		"@babel/polyfill": "latest",
		"@date-io/moment": "latest",
		"@emotion/react": "latest",
		"@emotion/styled": "latest",
		"moment": "latest",
		"react": "latest",
		"react-dom": "latest"
	},
	"devDependencies": {
		"@babel/cli": "latest",
		"@babel/core": "latest",
		"@babel/preset-env": "latest",
		"@babel/preset-react": "latest",
		"core-js": "latest"
	},
	"scripts": {
		"build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
		"dev": "npm run build && npm link",
		"deploy": "npm run build && npm publish"
	}
}
